{
  "expo": {
    "name": "qurizm",
    "slug": "qurizm",
    "version": "1.3.1",
    "orientation": "portrait",
    "sdkVersion": "52.0.0",
    "icon": "./assets/images/clogo.png",
   "scheme": "exp+godori",
    "userInterfaceStyle": "light",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/images/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "usesAppleSignIn": true,
      "supportsTablet": true,
      "bundleIdentifier": "com.test.qurizm",
      "buildNumber": "1.0.5",
      "infoPlist": {
        "CFBundleDisplayName": "Curizm",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
"android": {
  "package": "com.test.qurizm",
  "versionCode": 10,
  "adaptiveIcon": {
    "foregroundImage": "./assets/images/clogo.png",
    "backgroundColor": "#ffffff"
  },
  "targetSdkVersion": 35,
  "compileSdkVersion": 35,
  "edgeToEdgeEnabled": true
},

    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/clogo.png"
    },
    "plugins": [
      "expo-font",
      "expo-router",
      "expo-secure-store",
      "expo-apple-authentication",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-asset"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "bac81980-d4fd-4b31-9f0a-c0fb4ed364e6"
      }
    }
  }
}
