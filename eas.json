{
  "cli": {
    "version": ">= 16.18.0",
    "promptToConfigurePushNotifications": false
  },

  "build": {
    "internal": {
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "env": {
          "EXPO_ANDROID_TARGET_SDK_VERSION": "35"
        }
      }
    },

    "production": {
      "distribution": "store",
      "android": {
        "buildType": "app-bundle",
        "env": {
          "EXPO_ANDROID_TARGET_SDK_VERSION": "35"
        }
      }
    },

    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },

    "preview": {
      "distribution": "internal"
    }
  },

  "submit": {
    "production": {}
  }
}
