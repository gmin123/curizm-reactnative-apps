{
  "cli": {
    "version": ">= 16.18.0",
    "promptToConfigurePushNotifications": false
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "android": {
        "buildType": "app-bundle",
        "gradleCommand": ":app:bundleRelease",
        "env": {
          "EXPO_ANDROID_TARGET_SDK_VERSION": "35"
        }
      },
      "distribution": "store"
    }
  },
  "submit": {
    "production": {}
  }
}
